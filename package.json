{
  "name": "mate.js",
  "version": "0.0.0",
  "scripts": {
    "build": "jspm bundle lib/main dist/main.js",
    "doc": "typedoc --out .build/doc --readme README.md ./lib",
    "postdoc": "cpx icon.png .build/doc",
    "start": "http-server",
    "watch": "gulp",
    "pretest": "rimraf .build && tsc -p .",
    "test": "nyc mocha --reporter list",
    "precitest": "rimraf .build && tsc -p .",
    "citest": "nyc mocha --reporter mocha-junit-reporter",
    "postcitest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "postinstall": "typings install"
  },
  "nyc": {
    "exclude": [
      "test",
      ".build/test",
      "node_modules"
    ]
  },
  "jspm": {
    "format": "es6",
    "dependencies": {
      "lodash": "npm:lodash@^4.16.6",
      "object-hash": "npm:object-hash@^1.1.5"
    },
    "devDependencies": {
      "tsc": "npm:tsc@^1.20150623.0",
      "typescript": "npm:typescript@^1.6.2"
    }
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "cpx": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-mocha": "^3.0.1",
    "gulp-typescript": "^3.1.2",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.10",
    "mocha": "^3.1.2",
    "mocha-junit-reporter": "^1.12.1",
    "mocha-typescript": "^1.0.11",
    "nyc": "^8.4.0",
    "rimraf": "^2.5.4",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.7",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "object-hash": "^1.1.5"
  }
}
